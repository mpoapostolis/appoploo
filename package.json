{
  "name": "appoploo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "deploy": "turbo run build --scope=apps"
  },
  "devDependencies": {
    "@types/mapbox-gl": "^2.7.3",
    "autoprefixer": "^10.4.7",
    "eslint-config-custom": "latest",
    "postcss": "^8.4.14",
    "prettier": "latest",
    "tailwindcss": "^3.1.3",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@7.20.3",
  "dependencies": {
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "daisyui": "^2.15.4",
    "mapbox-gl": "^2.9.1"
  }
}
